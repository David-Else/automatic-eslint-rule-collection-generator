{
  "name": "app",
  "version": "1.0.0",
  "description": "Typescript template with Deno and Rollup bundling support",
  "main": "src/mod.ts",
  "scripts": {
    "run-deno-app": "deno -A --importmap=./import_map.json mod.ts",
    "test": "npm run clear-console && deno test ./ -A --importmap=./import_map.json",
    "lint": "npm run clear-console && eslint --cache --ext .ts src/",
    "clear-console": "deno eval \"console.clear()\""
  },
  "author": "https://www.elsewebdevelopment.com/",
  "license": "gpl-3.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "typescript": "^3.7.5"
  }
}
