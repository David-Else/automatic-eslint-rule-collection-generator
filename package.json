{
  "name": "app",
  "version": "1.0.0",
  "description": "Typescript template with Deno and Rollup bundling support",
  "main": "src/mod.ts",
  "scripts": {
    "run-deno-app": "deno run --allow-run --allow-write mod.ts",
    "test": "npm run clear-console && deno test --allow-run ./mod_test.ts",
    "lint": "npm run clear-console && eslint --cache --ext .ts ./",
    "clear-console": "deno eval \"console.clear()\""
  },
  "author": "https://www.elsewebdevelopment.com/",
  "license": "gpl-3.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "typescript": "^3.9.6"
  }
}
